{
  "name": "jest-a11y",
  "version": "1.1.1",
  "description": "Custom Jest matchers for testing accessibility",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && rollup -c config/rollup.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veiko/jest-a11y.git"
  },
  "keywords": [
    "a11y",
    "jest",
    "testing"
  ],
  "author": "Veronica Carrillo-Marquez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/veiko/jest-a11y/issues"
  },
  "homepage": "https://github.com/veiko/jest-a11y#readme",
  "dependencies": {
    "@testing-library/dom": "^8.17.1",
    "@testing-library/user-event": "^14.4.3"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "build: ${nextRelease.gitTag}"
        }
      ]
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.5.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "resolutions": {
    "@rollup/plugin-node-resolve": "^13.0.6"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest-setup.js"
    ]
  }
}
